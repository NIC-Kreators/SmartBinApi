{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:8080",
        "Protocols": "Http1"
      }
    }
  },
  "Mqtt": {
    "Topics": ["alert/smoke", "telemetry"]
  },
  "AllowedHosts": "*"
}
